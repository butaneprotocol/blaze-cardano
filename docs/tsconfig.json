{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "types": ["@docusaurus/module-type-aliases", "node"],
    "baseUrl": "."
  },
  "include": [
    "src/**/*",
    "docusaurus.config.ts",
    "plugins/**/*",
    "theme/**/*",
    "src/**/*.d.ts"
  ],
  "exclude": ["node_modules", "build"]
}
