{
  "name": "@blaze-cardano/query",
  "version": "0.5.4",
  "description": "Blaze cardano emulator library",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts --external rxjs",
    "watch": "tsdown src/index.ts --format esm,cjs --dts --external rxjs --watch",
    "dev": "tsdown src/index.ts --format esm,cjs --watch --dts --external rxjs",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest",
    "build:types": "tsc -p tsconfig.docs.json",
    "api:extract": "api-extractor run --local --verbose",
    "api:md": "rimraf docs/api && api-documenter markdown -i etc -o docs/api",
    "api:prepare": "pnpm build:types && pnpm api:extract && pnpm api:md"
  },
  "devDependencies": {
    "@blaze-cardano/eslint-config": "workspace:*",
    "@blaze-cardano/tsconfig": "workspace:*",
    "@types/jest": "^29.5.5",
    "@types/json-bigint": "^1.0.4",
    "@types/ws": "^8.5.10",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.9",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@blaze-cardano/core": "workspace:*",
    "@blaze-cardano/jest-config": "workspace:*",
    "@blaze-cardano/ogmios": "workspace:*",
    "@cardano-ogmios/schema": "^6.6.1",
    "@cardanosolutions/json-bigint": "^1.0.2",
    "ws": "^8.17.1"
  }
}
