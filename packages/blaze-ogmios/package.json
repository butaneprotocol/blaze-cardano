{
  "name": "@blaze-cardano/ogmios",
  "version": "0.0.7",
  "description": "Blaze cardano ogmios library",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts --external rxjs",
    "dev": "tsdown src/index.ts --format esm,cjs --watch --dts --external rxjs",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build:types": "tsc -p tsconfig.docs.json",
    "api:extract": "api-extractor run --local --verbose",
    "api:md": "rimraf docs/api && api-documenter markdown -i etc -o docs/api"
  },
  "devDependencies": {
    "@blaze-cardano/eslint-config": "workspace:*",
    "@blaze-cardano/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsdown": "^0.14.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cardano-ogmios/schema": "^6.6.1",
    "isomorphic-ws": "^5.0.0"
  }
}
